package com.ecld.java.excercises;

public class NameShuffle {
    public static void main(String[] args) {
        System.out.println(nameShuffle("Hello World"));
    }

    public static String nameShuffle(String s) {
        String[] full = s.split(" ");
        String reversed = full[1] + " " + full[0];
        return reversed;
    }
}
